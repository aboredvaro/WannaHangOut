{
  "extends": "next/core-web-vitals",
  "rules": {
      "indent": [
          "error",
          "tab"
      ],
      "quotes": [
          "error",
          "single"
      ],
      "semi": [
          "error",
          "never"
      ],
      "no-multiple-empty-lines": [
          "warn",
          { "max": 1, "maxEOF": 1 }
      ],
      "@next/next/no-img-element": "off"
  }
}

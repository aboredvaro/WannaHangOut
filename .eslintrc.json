{
  "extends": "next/core-web-vitals",
  "rules": {
      "indent": [
          "error",
          "tab"
      ],
      "linebreak-style": [
          "error",
          "unix"
      ],
      "quotes": [
          "error",
          "single"
      ],
      "semi": [
          "error",
          "never"
      ],
      "no-multiple-empty-lines": [
          "error",
          { "max": 1, "maxEOF": 1 }
      ]
  }
}
